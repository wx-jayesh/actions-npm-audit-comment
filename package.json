    "url": "https://github.com/wx-jayesh/actions-npm-audit-comment/issues"
  },
  "homepage": "https://github.com/wx-jayesh/actions-npm-audit-comment#readme",
  "devDependencies": {
    "@types/node": "^14.14.33",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0"
  }
}
